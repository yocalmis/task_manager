<?xml version='1.0' encoding='utf-8'?>
<widget id="io.gorev_listesi" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <feature name="connectivity">
        <param name="android-package" value="com.appfeel.cordova.connectivity.Connectivity" />
    </feature>
    <feature name="AdMobAds">
        <param name="android-package" value="com.appfeel.cordova.admob.AdMobAds" />
    </feature>
    <feature name="Whitelist">
        <param name="android-package" value="org.apache.cordova.whitelist.WhitelistPlugin" />
        <param name="onload" value="true" />
    </feature>
    <name>gorev_listesi</name>
    <description>
        A sample Apache Cordova application that responds to the deviceready event.
    </description>
    <author email="dev@cordova.apache.org" href="http://cordova.io">
        Apache Cordova Team
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <allow-intent href="market:*" />
    <icon density="ldpi" height="57" platform="android" src="res/icon/android/aa.png" width="57" />
    <icon density="mdpi" height="57" platform="android" src="res/icon/android/aa.png" width="57" />
    <icon density="hdpi" height="57" platform="android" src="res/icon/android/aa.png" width="57" />
    <icon density="xhdpi" height="57" platform="android" src="res/icon/android/aa.png" width="57" />
    <icon density="xxhdpi" height="57" platform="android" src="res/icon/android/aa.png" width="57" />
    <icon density="xxxhdpi" height="57" platform="android" src="res/icon/android/aa.png" width="57" />
    <splash density="land-hdpi" src="res/screen/android/aa.png" />
    <splash density="land-ldpi" src="res/screen/android/aa.png" />
    <splash density="land-mdpi" src="res/screen/android/aa.png" />
    <splash density="land-xhdpi" src="res/screen/android/aa.png" />
    <splash density="port-hdpi" src="res/screen/android/aa.png" />
    <splash density="port-ldpi" src="res/screen/android/aa.png" />
    <splash density="port-mdpi" src="res/screen/android/aa.png" />
    <splash density="port-xhdpi" src="res/screen/android/aa.png" />
    <preference name="loglevel" value="DEBUG" />
</widget>
